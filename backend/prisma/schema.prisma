generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

enum Role {
  PMM
  LIDER
  TECNICO
  ESTAGIARIO
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String
  password  String
  role      Role     @default(TECNICO)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}